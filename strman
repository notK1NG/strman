function concat(a,b){
    return a + b;
}
function printfirst(a){
    return a[0];
}
function printLast(a){
    return a[-1];    
}
function slice4(a){
    return a.slice(0,4);
}

exports.concat = concat;
exports.printFirst = printFirst;
exports.printLast = printLast;
exports.slice4 = slice4;